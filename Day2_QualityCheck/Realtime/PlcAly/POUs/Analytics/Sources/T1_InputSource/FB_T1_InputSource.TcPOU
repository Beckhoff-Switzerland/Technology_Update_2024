    <Method Name="NewDataAvailable" Id="{4847d983-5d67-4dc7-921e-5497973d374e}" FolderPath="I_InputSource\">
      <Declaration><![CDATA[METHOD NewDataAvailable : BOOL
VAR_INPUT
