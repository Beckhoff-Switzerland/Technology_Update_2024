      <Declaration><![CDATA[PROPERTY eDataState : E_DataSourceState]]></Declaration>
      <Get Name="Get" Id="{12dd5f27-34f0-4511-952e-87b7af302fc7}">
        <Declaration><![CDATA[VAR
END_VAR]]></Declaration>
